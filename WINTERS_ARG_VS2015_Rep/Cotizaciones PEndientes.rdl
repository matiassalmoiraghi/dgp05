          <DataField>CRITERIO_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CRITERIO_2">
          <DataField>CRITERIO_2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
