    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SOPTYPE">
            <Value>=Parameters!SOPTYPE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LINEA">
            <Value>=Parameters!LINEA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ALERTA">
            <Value>=Parameters!ALERTA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FECHA">
            <Value>=Parameters!FECHA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ZONA">
            <Value>=Parameters!ZONA.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  CASE WHEN @ALERTA = 'ZONA'  THEN    RTRIM( LINEA.SALSTERR)        
                         WHEN  @ALERTA = 'SEGMENTO' THEN RTRIM( LINEA.USERDEF2)          
                         ELSE LINEA.USCATVLS_6 
               END  AGRUPACION,
              A.SOPNUMBE DOCUMENTO , 
              A.CUSTNMBR CLIENTE ,
              rtrim(A.CUSTNAME) NOMBRE,
               A.DOCDATE FECHA, 
              SUM(PRECIO)  PRECIO 
            ,SUM(COSTO) COSTO
            ,CASE WHEN @ALERTA = 'ZONA'  THEN LINEA.SLPRSNID
                           ELSE ' ' 
                           END PROVINCIA
            ,  CASE WHEN @ALERTA = 'SEGMENTO'  THEN       RTRIM( LINEA.SALSTERR)                                              ELSE   LINEA.USERDEF2  
              END   CRITERIO_1
            , CASE WHEN @ALERTA = 'LINEA'  THEN       RTRIM( LINEA.SALSTERR)                      
                                        ELSE   LINEA.USCATVLS_6          
                 END  CRITERIO_2
  FROM SOP10100 A 
              INNER JOIN ( 
                    SELECT    A.SOPTYPE, 
	                  A.SOPNUMBE, 
		C.USCATVLS_6 ,	
                                     D.SALSTERR ,	
          		D.SLPRSNID,
                                     D.USERDEF2,
		CASE WHEN B.REMPRICE != 0 		THEN 
                                             B.REMPRICE-ROUND((B.TRDISAMT/B.QUANTITY)*B.QTYREMAI, 2) 
		        ELSE	0   END PRECIO
	   	,CASE WHEN B.REMPRICE != 0  
			THEN ROUND((B.EXTDCOST/B.QUANTITY)*B.QTYREMAI, 2) 
			ELSE 0 	END COSTO
	FROM SOP10100 A 
                              INNER JOIN RM00101 D ON A.CUSTNMBR = D.CUSTNMBR
		INNER JOIN SOP10200 B ON A.SOPTYPE = B.SOPTYPE AND 
                                                                                      A.SOPNUMBE = B.SOPNUMBE
		INNER JOIN IV00101 C ON B.ITEMNMBR = C.ITEMNMBR
	WHERE A.SOPTYPE = @SOPTYPE
   	  AND A.DOCDATE &gt;= '20121101'  
AND A.DOCDATE &lt;= CONVERT(CHAR(8),  @FECHA, 112)
	  AND C.USCATVLS_6  in(@LINEA) 
	  AND D.SALSTERR in(@ZONA)
 	) 
	LINEA ON A.SOPTYPE = LINEA.SOPTYPE AND A.SOPNUMBE = LINEA.SOPNUMBE 
  GROUP BY 	  A.SOPTYPE
		, A.SOPNUMBE
		, A.CUSTNMBR
		, RTRIM(A.CUSTNMBR) , A.CUSTNAME 
                                     ,  USCATVLS_6         	
                                     ,  LINEA.SLPRSNID
                                      ,  LINEA.SALSTERR
                                     , LINEA.USERDEF2  
		,  A.DOCDATE
                                     ,  CONVERT(DECIMAL(10,2), A.TAXAMNT)
                                      , CONVERT(DECIMAL(10,2), A.DOCAMNT)      </CommandText>
      </Query>
      <Fields>
        <Field Name="AGRUPACION">
          <DataField>AGRUPACION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCUMENTO">
          <DataField>DOCUMENTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CLIENTE">
          <DataField>CLIENTE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NOMBRE">
          <DataField>NOMBRE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FECHA">
          <DataField>FECHA</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PRECIO">
          <DataField>PRECIO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="COSTO">
          <DataField>COSTO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
